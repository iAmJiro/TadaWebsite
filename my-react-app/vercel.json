{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "framework": {
    "name": "vite"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "ignoreBuildCache": true
}
